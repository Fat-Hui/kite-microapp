.container{
  width: 100%;
}

.tab-header{
  display: flex;
  width: 100%;
  height: 75rpx;
  padding: 10rpx 0;
}

.tabItem{
  flex-shrink: 0;
  height: 45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 5rpx 10rpx;
  margin: 0 10rpx;
  font-size: 32rpx;
}

.activeItem{
  background-color: #aaa;
  border-radius: 20rpx;
}